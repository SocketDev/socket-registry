{
  "name": "@socketsecurity/registry",
  "version": "2.0.0",
  "license": "MIT",
  "description": "Socket Registry - Core utilities and infrastructure for Socket.dev security tools",
  "keywords": [
    "Socket.dev",
    "security",
    "registry",
    "npm",
    "packages"
  ],
  "homepage": "https://github.com/SocketDev/socket-registry",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SocketDev/socket-registry.git",
    "directory": "registry"
  },
  "author": {
    "name": "Socket Inc",
    "email": "eng@socket.dev",
    "url": "https://socket.dev"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "default": "./dist/types.js"
    },
    "./extensions.json": "./extensions.json",
    "./manifest.json": "./manifest.json",
    "./package.json": "./package.json",
    "./tsconfig.dts.json": "./tsconfig.dts.json",
    "./tsconfig.json": "./tsconfig.json"
  },
  "files": [
    "dist/**/*.js",
    "dist/**/*.d.ts",
    "extensions.json",
    "manifest.json",
    "CHANGELOG.md"
  ],
  "engines": {
    "node": ">=18"
  },
  "sideEffects": false,
  "scripts": {
    "build": "pnpm run clean && pnpm run build:js && pnpm run build:types",
    "build:js": "node scripts/build-js.mjs",
    "build:types": "tsgo --project tsconfig.dts.json --declaration --emitDeclarationOnly",
    "build:watch": "node scripts/build-js.mjs --watch",
    "check": "tsgo --noEmit",
    "clean": "del-cli dist '**/*.tsbuildinfo'",
    "dev": "pnpm run build:watch",
    "prepublishOnly": "pnpm run build",
    "test": "vitest",
    "type-ci": "pnpm run check"
  },
  "dependencies": {
    "semver": "7.7.2",
    "which": "5.0.0",
    "yoctocolors-cjs": "2.1.3"
  },
  "devDependencies": {
    "@babel/core": "7.28.4",
    "@babel/parser": "7.28.4",
    "@babel/traverse": "7.28.4",
    "@babel/types": "7.28.4",
    "@inquirer/confirm": "5.1.16",
    "@inquirer/input": "4.2.2",
    "@inquirer/password": "4.0.18",
    "@inquirer/search": "3.1.1",
    "@inquirer/select": "4.3.2",
    "@npmcli/package-json": "7.0.0",
    "@npmcli/promise-spawn": "8.0.3",
    "@socketregistry/is-unicode-supported": "workspace:*",
    "@socketregistry/packageurl-js": "1.3.0",
    "@socketregistry/yocto-spinner": "workspace:*",
    "@types/node": "24.6.2",
    "@yarnpkg/extensions": "2.0.6",
    "cacache": "20.0.1",
    "debug": "4.4.3",
    "del": "8.0.1",
    "del-cli": "6.0.0",
    "fast-glob": "3.3.3",
    "fast-sort": "3.4.1",
    "get-east-asian-width": "1.3.0",
    "libnpmpack": "9.0.9",
    "magic-string": "0.30.17",
    "make-fetch-happen": "15.0.2",
    "normalize-package-data": "8.0.0",
    "npm-package-arg": "13.0.0",
    "pacote": "21.0.1",
    "picomatch": "2.3.1",
    "semver": "7.7.2",
    "spdx-correct": "3.2.0",
    "spdx-expression-parse": "4.0.0",
    "streaming-iterables": "8.0.1",
    "typescript": "5.7.3",
    "validate-npm-package-name": "6.0.2",
    "vitest": "3.2.4",
    "which": "5.0.0",
    "yargs-parser": "22.0.0",
    "yoctocolors-cjs": "2.1.3",
    "zod": "4.1.12"
  },
  "peerDependencies": {
    "typescript": ">=5.0.0"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    }
  }
}
