{
  "name": "@socketsecurity/registry",
  "version": "1.6.0",
  "license": "MIT",
  "description": "Socket Registry - Core utilities and infrastructure for Socket.dev security tools",
  "keywords": [
    "Socket.dev",
    "security",
    "registry",
    "npm",
    "packages"
  ],
  "homepage": "https://github.com/SocketDev/socket-registry",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SocketDev/socket-registry.git",
    "directory": "registry"
  },
  "author": {
    "name": "Socket Inc",
    "email": "eng@socket.dev",
    "url": "https://socket.dev"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./constants": {
      "types": "./dist/constants/index.d.ts",
      "import": "./dist/constants/index.js"
    },
    "./constants/static": {
      "types": "./dist/constants/static.d.ts",
      "import": "./dist/constants/static.js"
    },
    "./constants/lazy": {
      "types": "./dist/constants/lazy.d.ts",
      "import": "./dist/constants/lazy.js"
    },
    "./constants/env": {
      "types": "./dist/constants/env.d.ts",
      "import": "./dist/constants/env.js"
    },
    "./lib/constants": {
      "types": "./dist/lib/constants/index.d.ts",
      "import": "./dist/lib/constants/index.js"
    },
    "./utils": {
      "types": "./dist/utils.d.ts",
      "import": "./dist/utils.js"
    },
    "./packages": {
      "types": "./dist/packages.d.ts",
      "import": "./dist/packages.js"
    },
    "./cli": {
      "types": "./dist/cli.d.ts",
      "import": "./dist/cli.js"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "import": "./dist/types.js"
    },
    "./lib/packages": {
      "types": "./dist/lib/packages/operations.d.ts",
      "import": "./dist/lib/packages/operations.js"
    },
    "./lib/logger": {
      "types": "./dist/lib/logger.d.ts",
      "import": "./dist/lib/logger.js"
    },
    "./lib/objects": {
      "types": "./dist/lib/objects.d.ts",
      "import": "./dist/lib/objects.js"
    },
    "./lib/strings": {
      "types": "./dist/lib/strings.d.ts",
      "import": "./dist/lib/strings.js"
    },
    "./package.json": "./package.json"
  },
  "imports": {
    "#constants": "./dist/constants.js",
    "#utils": "./dist/utils.js",
    "#packages": "./dist/packages.js",
    "#cli": "./dist/cli.js",
    "#types": "./dist/types.js"
  },
  "files": [
    "dist",
    "manifest.json",
    "extensions.json",
    "CHANGELOG.md"
  ],
  "engines": {
    "node": ">=20.0.0"
  },
  "sideEffects": false,
  "scripts": {
    "build": "pnpm run clean && pnpm run build:ts && pnpm run build:types && pnpm run build:externals && pnpm run fix:exports && pnpm run fix:imports",
    "build:ts": "tsgo --project tsconfig.json",
    "build:types": "tsgo --project tsconfig.dts.json --declaration --emitDeclarationOnly",
    "build:externals": "node scripts/build-externals.mjs",
    "fix:exports": "node scripts/fix-commonjs-exports.mjs",
    "fix:imports": "node scripts/fix-default-imports.mjs",
    "clean": "del-cli dist '**/*.tsbuildinfo'",
    "check": "tsgo --noEmit",
    "dev": "tsgo --watch",
    "prepublishOnly": "pnpm run build",
    "test": "vitest",
    "type-ci": "pnpm run check"
  },
  "dependencies": {
    "semver": "7.7.2",
    "which": "5.0.0",
    "yoctocolors-cjs": "2.1.3"
  },
  "devDependencies": {
    "@inquirer/confirm": "5.1.16",
    "@inquirer/input": "4.2.2",
    "@inquirer/password": "4.0.18",
    "@inquirer/search": "3.1.1",
    "@inquirer/select": "4.3.2",
    "@socketregistry/packageurl-js": "1.3.0",
    "@socketregistry/yocto-spinner": "1.0.24",
    "@types/node": "24.6.2",
    "del": "8.0.1",
    "del-cli": "6.0.0",
    "typescript": "5.7.3",
    "vitest": "3.2.4"
  },
  "peerDependencies": {
    "typescript": ">=5.0.0"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    }
  }
}