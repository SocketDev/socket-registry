{
  "name": "@socketregistry/<%= it.name %>",
  "version": "<%= it.version %>",
  "license": "MIT",
  "description": "Socket.dev optimized package override for <%= it.originalName %>",
  "keywords": [
    "Socket.dev",
    "package-overrides"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SocketDev/socket-registry-js.git",
    "directory": "packages/npm/<%= it.name %>"
  },
  "main": "./index.js",
  "sideEffects": false,
  "engines": {
    "node": "<%~ it.engines.node %>"
  },
  "files": [
    "*.d.ts",
    "*.js"
  ],
  "socket": {
    "categories": "//_ <%~ JSON.stringify(it.categories) %>"
  }
}
