{
  "name": "@socketregistry/<%= it.name %>",
  "version": "<%= it.version %>",
  "license": "MIT",
  "description": "Socket.dev optimized package override for <%= it.originalName %>",
  "keywords": [
    "Socket.dev",
    "package-overrides"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/SocketDev/socket-registry-js",
    "directory": "packages/npm/<%= it.name %>"
  },
  "type": "module",
  "exports": {
    "module-sync": "./node.js",
    "require": "./node.cjs",
    "import": "./node.js",
    "default": "./index.js"
  },
  "sideEffects": false,
  "engines": {
    "node": "<%~ it.node_range %>"
  },
  "files": [
    "*.d.ts",
    "*.cjs",
    "*.js",
    "*.original{.*,}"
  ],
  "socket": {
    "categories": ["<%~ it.categories %>"]
  }
}
